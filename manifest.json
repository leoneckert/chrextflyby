{
  "name": "imgSwap",
  "version": "1.0",
  "manifest_version": 2,
  "description": "",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["inject.js"],
          "run_at": "document_start"
        }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
      "manifest.json"
  ]
}
